.container#logged-in-main
  .row
    .col-lg-12
      .unobtrusive-flash-container
  .row
    .col-lg-8.col-lg-offset-2
      = form_for(@eligibility) do |f|
        =f.text_field :email, placeholder: 'Your email address', class: "input-lg"
        =f.submit "Check eligibility", class: "btn btn-info"